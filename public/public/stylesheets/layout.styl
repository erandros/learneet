body
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
  margin 0px
a
  color: #00B7FF
  text-decoration: none;

#main
  margin-top: 10px
  padding: 20px

button
  font-family Arial,sans-serif
  color white
  border 1px solid transparent
  border-radius 3px
  padding 6px 10px 6px 10px
  border-style solid
  font-weight bold
  font-size 11px
  background-color #428bca
  cursor pointer

.childContainer
  width 1050px
  margin 0 auto

.marginBottom
  margin-bottom 15px